{% extends 'base.html.twig' %}

{% block title %}Hello Users!{% endblock %}

{% set include_navigation_bar = true %}

{% block body %}

   {% if users|length > 0 %}

      {% for user in users %}
         <div>{{ user.username }}</div>
      {% endfor %}
      
   {% else %}
      No users found!
   {% endif %}

{% endblock %}